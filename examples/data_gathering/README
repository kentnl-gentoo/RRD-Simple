# Send from the client to a remote server
*/4 * * * * sleep 20; /usr/bin/perl /home/nicolaw/svn/RRD-Simple/examples/data_gathering/rrd-client.pl 2>/dev/null | /usr/bin/POST http://merry.ntl.tfb.net/rrd-cgi-bin/rrd-server.cgi >/dev/null

# Pipe client to server
*/4 * * * * sleep 20; /home/system/rrd/bin/rrd-client.pl | /home/system/rrd/bin/rrd-server.pl -u merry.tfb.net

# Send over the network using netcat
/home/system/rrd/bin/rrd-client.pl | nc -p 4444 merry.tfb.net
nc -l -p 4444 | /home/system/rrd/bin/rrd-server.pl -u arwen.tfb.net


